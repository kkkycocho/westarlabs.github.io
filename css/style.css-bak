}

.am-topbar-nav > li > a:after,
.am-topbar-nav > li > a:link,
.am-topbar-nav > li > a:visited,
.am-topbar-nav > li > a:hover,
.am-topbar-nav > li > a:active {
    content: none;
    background: none;
}

.am-topbar-nav > li.am-active > a {
    border-bottom: 2px solid #25D0ED;
    color: #25D0ED;
}

.am-topbar-brand, .am-nav li, .am-topbar-nav > li > a:hover {
    background: none;
}

.am-topbar-nav > li > a:visited {
